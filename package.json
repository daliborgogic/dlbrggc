{
  "name": "daliborgogic.com",
  "version": "1.0.0",
  "description": "###",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "date-fns": "^2.0.0-alpha.24",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.7.1",
    "nuxt": "^2.2.0",
    "r2": "^2.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "plugins": [
      "vue"
    ],
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "no-console": "error",
      "vue/max-attributes-per-line": "off"
    }
  }
}
